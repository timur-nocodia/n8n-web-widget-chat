# Database Configuration
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/chat_proxy
REDIS_URL=redis://localhost:6379/0

# Security Keys (CHANGE THESE IN PRODUCTION!)
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production
SESSION_SECRET_KEY=your-session-secret-key-change-this-in-production

# n8n Integration
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/your-webhook-id/chat
N8N_API_KEY=optional-api-key-if-needed

# CORS Configuration
# Comma-separated list of allowed origins
ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com

# Logging
LOG_LEVEL=WARNING  # DEBUG, INFO, WARNING, ERROR, CRITICAL

# Optional: Rate Limiting
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000